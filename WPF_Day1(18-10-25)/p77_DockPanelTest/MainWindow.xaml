<Window x:Class="p77_DockPanelTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:p77_DockPanelTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <Button DockPanel.Dock="Left"> Left Buttom</Button>
        <Button DockPanel.Dock="Right">Right Buttom</Button>
        <Button DockPanel.Dock="Top"> Top Buttom</Button>
        <Button DockPanel.Dock="Bottom"> Bottom Buttom</Button>
        <Button>Non Docking</Button>
    </DockPanel>
</Window>
